using System;

namespace program_1 {
	internal class Program {
		static void Main() {
			string word;
			Console.Write("enter word: ");
			word = Console.ReadLine();
			string[] modified = new string[word.Length];
			int j = -1;
			for(int i = 0; i < word.Length; i+=2) {
				j++;
				modified[j] = Convert.ToString(word[i]);
			}
			for(int i = 0; i < modified.Length; i++) {
				Console.Write(modified[i]);
			}
			Console.ReadKey();
		}
	}
}
